<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://skc3.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>skc3.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>skc3</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$skc3</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="SKC.Lib.Service.RespositoryContext" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:maestrodb.database.windows.net,1433;Initial Catalog=skcdb;User ID=maestro_dba;Password=Admin@123456" Name="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="SKC.Lib.Service.RespositoryContext, SKC.Lib.Service" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="DefaultConnection" Order="2" Enabled="False">
          <Destination Path="Data Source=tcp:maestrodb.database.windows.net,1433;Initial Catalog=skcdb;User ID=maestro_dba;Password=Admin@123456" Name="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="SuperKidCity.Models.ApplicationDbContext, SuperKidCity" MigrationConfiguration="SuperKidCity.Migrations.Configuration, SuperKidCity" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:maestrodb.database.windows.net,1433;Initial Catalog=skcdb;User ID=maestro_dba;Password=Admin@123456</ParameterValue>
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)SKC.Lib.Service.RespositoryContext-Web.config Connection String">
      <ParameterValue>Data Source=tcp:maestrodb.database.windows.net,1433;Initial Catalog=skcdb;User ID=maestro_dba;Password=Admin@123456</ParameterValue>
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="SKC.Lib.Service.RespositoryContext" />
  </ItemGroup>
</Project>